{
  "expo": {
    "name": "WicBuy",
    "slug": "wicbuy",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wicbuy.app",
      "infoPlist": {
        "NSCameraUsageDescription": "WicBuy needs camera access to scan product barcodes and verify WIC eligibility.",
        "NSMicrophoneUsageDescription": "WicBuy needs microphone access for audio feedback features."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.wicbuy.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow WicBuy to scan product barcodes to verify WIC eligibility."
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://wic-food-production.up.railway.app",
      "eas": {
        "projectId": "f5402211-e58e-47ba-b893-f6f7e279a0f1"
      }
    },
    "owner": "ethanlma"
  }
}
